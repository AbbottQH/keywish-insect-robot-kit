const int distancePin = A1; 
int dl;
void setup()
{
  Serial.begin(9600);
  pinMode(distancePin, INPUT); 
}

void loop()
{
  dl = analogRead(distancePin); 
  Serial.print("distance:");
  Serial.println(dl);
}


